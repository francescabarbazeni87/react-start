{
  "name": "react-start",
  "version": "0.0.0",
  "description": "react starter kit",
  "repository": "https://github.com/marcorigodanzo/react-start",
  "main": "src/App.js",
  "scripts": {
    
    "build": "browserify --debug src/App.js > dist/bundle.js",
    "build-dist": "set NODE_ENV=production&& browserify src/App.js | uglifyjs -cm > dist/bundle.min.js",
    "build-dist-linux": "NODE_ENV=production browserify src/App.js | uglifyjs -cm > dist/bundle.min.js",
    "watch": "watchify --debug src/App.js -o dist/bundle.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "author": "Marco Rigodanzo",
  "license": "MIT",
  "dependencies": {
    "flux": "^2.0.0",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.12.0",
    "body-parser": "^1.6.3",
    "es5-shim": "^4.0.1",
    "es6-promise": "^1.0.0",
    "event-emitter": "^0.3.1",
    "express": "^4.7.4",
    "lodash-node": "^2.4.1",
    "merge": "^1.1.3",
    "node-jsx": "^0.12.0",
    "react-router": "0.11.4",
    "supertest": "^0.13.0"
  },
  "devDependencies": {
    "browserify": "^7.0.0",
    "casperjs": "^1.1.0-beta3",
    "cheerio": "^0.17.0",
    "d3": "^3.5.2",
    "envify": "^3.2.0",
    "expect.js": "^0.3.1",
    "jasmine-react-helpers": "^0.2.0",
    "karma": "^0.12.21",
    "karma-bro": "^0.6.2",
    "karma-chrome-launcher": "^0.1.4",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.1.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "mocha": "^1.21.4",
    "react-bootstrap": "^0.13.0",
    "react-tools": "^0.11.1",
    "reactify": "^0.17.1",
    "request": "^2.40.0",
    "rewireify": "0.0.9",
    "uglify-js": "^2.4.16",
    "watchify": "^2.1.1"
  }
}
